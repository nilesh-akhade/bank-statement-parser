{
    "sbi": {
        "name": "SBI",
        "identifier": "GSTIN of SBI Card",
        "crop_box": [10, 422, 430, 675],
        "pattern": {
            "regex": "(\\d{2} \\w{3} \\d{2}) (.*?) (\\d{1,3}(?:,\\d{3})*\\.?\\d{0,2}) ([CD])",
            "date_format": "%d %b %y",
            "date_group": 1,
            "desc_group": 2,
            "amount_group": 3,
            "type_group": 4,
            "credit_identifier": "C",
            "debit_identifier": "D",
            "credit_suffix": null
        }
    },
    "hdfc": {
        "name": "HDFC",
        "identifier": "HDFC Bank Credit Cards GSTIN",
        "crop_box": [10, 200, 430, 600],
        "pattern": {
            "regex": "(\\d{2}/\\d{2}/\\d{4})\\s+((?:\\d+%|[A-Za-z]).*?)\\s+((?:\\d{1,3}(?:,\\d{3})*\\.?\\d{0,2})(?:\\s*Cr)?)\\s*$",
            "date_format": "%d/%m/%Y",
            "date_group": 1,
            "desc_group": 2,
            "amount_group": 3,
            "type_group": null,
            "credit_identifier": "C",
            "debit_identifier": "D",
            "credit_suffix": "Cr"
        }
    }
}